{
  "name": "gas-webpagetest",
  "private": true,
  "scripts": {
    "deploy": "run-s clean build push",
    "push": "clasp push",
    "clean": "rimraf dist",
    "build": "webpack",
    "postbuild": "cpx appsscript.json dist",
    "prettier": "prettier --write \"**/*.{js,ts}\"",
    "precommit": "lint-staged",
    "postcommit": "git reset"
  },
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "~1.4",
    "@types/google-apps-script": "^0.0.26",
    "core-js": "^2.5.7",
    "cpx": "^1.5.0",
    "dotenv-webpack": "^1.5.7",
    "gas-webpack-plugin": "^0.3.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "semi": false,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
